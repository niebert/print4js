    "webpack": "4.33.0",
    "webpack-cli": "3.3.3",
    "webpack-dev-server": "3.7.1"
  },
  "scripts": {
    "build": "webpack --mode development --progress --hide-modules --devtool source-map;node ./copy4build.js; node build.js; doctoc ./README.md",
    "test": "standard && karma start",
    "dev": "webpack --mode development --progress --hide-modules --devtool source-map",
    "watch": "webpack --mode development --watch --progress --hide-modules",
    "production": "webpack --mode production --progress --hide-modules",
    "coverage": "open coverage/lcov-report/index.html",
    "coveralls": "cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "start": "webpack-dev-server --public http://localhost:8080/test/manual --open"
  },
  "build": {
    "readme": "README_build.md",
    "html": "docs/demo7_print_handlebars4print.html",
    "css": "docs/css/print4js.css",
    "htmlsrc": "docs/index_src_libs.html"
  },
  "author": "Rodrigo Vieira <rodrigo@crabbly.com>",
  "gituser": "niebert",
  "gitdomain": "github",
  "bugs": {
    "url": "https://www.github.com/niebert/print4js/issues"
  },
  "standard": {
    "ignore": [
      "/dist/print.js"
    ],
    "env": {
      "browser": true,
      "jasmine": true
    }
  },
  "keywords": [
    "printjs",
    "print.js",
    "print-js"
  ]
